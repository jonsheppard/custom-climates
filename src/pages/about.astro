---
import Layout from '../layouts/Layout.astro';

const title = "About Us | Custom Climates";
const description = "Learn about the Custom Climates story, our values, and our commitment to providing reliable, innovative climate control solutions to our customers.";
---

<Layout title={title} description={description}>
  <main class="py-20 px-8 text-center">
    <h1 class="text-4xl font-bold">About Custom Climates</h1>
    <p class="mt-4 text-lg text-gray-400">
      Learn about our story, our values, and our commitment to our customers.
    </p>
  </main>
</Layout> 