---
import Layout from '../layouts/Layout.astro';

const title = "Our Equipment | Custom Climates";
const description = "Explore our high-performance, jobsite-ready equipment, including climate-controlled trailers, containers, and power generation units.";
---

<Layout title={title} description={description}>
  <main class="py-20 px-8 text-center">
    <h1 class="text-4xl font-bold">Our Equipment</h1>
    <p class="mt-4 text-lg text-gray-400">
      High-performance equipment, built for the jobsite. This page will showcase our trailers and containers.
    </p>
  </main>
</Layout> 