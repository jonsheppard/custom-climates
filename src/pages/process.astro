---
import Layout from '../layouts/Layout.astro';

const title = "Our Process | Custom Climates";
const description = "Learn about our seamless process, from first call and consultation to delivery and 24/7 support, ensuring your project stays on track.";
---

<Layout title={title} description={description}>
  <main class="py-20 px-8 text-center">
    <h1 class="text-4xl font-bold">Our Process</h1>
    <p class="mt-4 text-lg text-gray-400">
      Seamless from first call to final delivery. This page will detail our 5-step journey.
    </p>
  </main>
</Layout> 